       function doUpdate(id,a){

              if(a==0){
				   var json10 = {
				        "sendtype":10052,
				        "data":{
				        "companylicensetypeId": id,
				        "companylicensetypeName": $("#meg2").val(),
				        "companylicensetypeStatus": 0,	
			  				        			        			        
				        }
				    };  
                $.ajax({
			           async:false,                
				       type:"post",                         
				       url:"mobile!mobile.action",	 
				       dataType:"json",   
		      		   data:"&para="+JSON.stringify(json10),
				       success:function(d){	
			
		                          window.location.href="list_licensetype.jsp";
			                                                                            
			                    }	                                  		           		                  	     		     
		
				       });				                
              }else{
              
				   var json11 = {
