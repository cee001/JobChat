	     function getdata(m){  
	    
	     			 var n= document.getElementById("nid").value;  
		             if(m==1 & n!=1){
		             n=n-2;
		             }
			    var json ={
			    "sendtype":10050,
			    "data":{
			    "pn":n,
			    "ps":25,
			    }};
				    
	           $.ajax({   
			         async:false,  	           	       		
				     type:"post",
		             url:"mobile!mobile.action",	 				      
				     dataType:"json",
      		         data:"para="+JSON.stringify(json), 
				     success:function(d){
				     		     $("#dmsgid1").html("");
				                 $("#dmsgid2").html("");
				                 $("#dmsgid3").html("");
			
					     var data1=d.data;
					     var data2=data1.list;
   				     
				         var pn=d.pn;
