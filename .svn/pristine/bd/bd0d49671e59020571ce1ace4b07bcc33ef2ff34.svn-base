				         document.getElementById("nid").value=n;
                         var count=d.count;
		                 lastPager=Math.ceil(count/25);	
		                 if(n==2){									       
					  	   $("#d12").html('<div>上一页 </div>');  				     
					     }else{						
					  	   $("#d12").html('<div><a onclick="getdata(1)" href="#">'+"上一页"+'</a></div>');  				     
					     }
					     if(n<=lastPager){
									      			     
					  	   $("#d10").html('<div><a onclick="getdata(0)" href="#">'+"下一页"+'</a>('+pn+'/'+lastPager+')</div>');  				     
					     }else{		
					       
