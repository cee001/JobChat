				   });    				                                              
			    } 
			    
			    });
			    }
 
              var b= 1;	 
           
       function showUpdate(id,a){
              
              if(b!=2){

	              $("#megname").val($("#n"+id).text()); 
	              $("#n"+id).html('<table height="30px"><tr><td><input id="meg2" style="float: left;margin-top:3px;" class="list_input3" type="text" value="'+$("#n"+id).text()+'"/></td>'
